<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.elevensteps">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_elevensteps"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_elevensteps"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MenuRutasActivity">
            <intent-filter>
                <action android:name="android.intent.action.SECOND" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".DescripcionRuta" />
        <activity android:name=".PuntoDeInteres" />
        <activity android:name=".Mapa"></activity>
    </application>

</manifest>